{
  "name": "EasycleanBrowsing.v.1.0",
  "description": "브라우저상 남아있는 데이터 정보들을 모두 매우 간편하게 단축키 하나로 삭제합니다.",
  "background": { "scripts": ["js/back-paper.js"]},
  "permissions": ["browsingData"],
  "browser_action": {
    "default_icon":"images/icon-cache_false.png",
    "default_popup":"paper.html"
  },
  "commands":{
    "toggle-feature":
    {
      "suggested_key":
      {
        "default": "Ctrl+Shift+M",
        "mac": "MacCtrl+Shift+M"
      },
      "description": "remove data key"
    }
  },
  "version": "1.0",
  "manifest_version": 2
}